! keycode [keycode in hex] = keysym
clear lock
! map numlock to comma
keycode 0x4D = 0x2C
! map keypad keys for numlock=off to numlock=on
! to override case where numlock=off
keycode 0x5A = 0x30
keycode 0x57 = 0x31
keycode 0x58 = 0x32
keycode 0x59 = 0x33
keycode 0x53 = 0x34
keycode 0x54 = 0x35
keycode 0x55 = 0x36
keycode 0x4F = 0x37
keycode 0x50 = 0x38
keycode 0x51 = 0x39
keycode 0x5B = 0x2E
keycode 0x6A = 0x2F
keycode 0x3F = 0x2A
keycode 0x52 = 0x2D
keycode 0x56 = 0x2B
keycode 0x68 = 0xFF0D
! map escape to capslock
keycode 0x42 = Escape
! map menu key to mod
keycode 0x87 = Super_L
! map esc to mod
keycode 0x09 = Super_L
! extra
keycode 0x79 = F13
keycode 0x7A = F14
keycode 0x7B = F15
